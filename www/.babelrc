{
	"presets": [
		[
			"babel-preset-gatsby", {
				"browserslist": "last 1 chrome versions"
			}
		],
		[
			"@babel/preset-typescript", {
			"isTSX": true,
			"allExtensions": true,
		}
	]],
	"plugins": [
		["@babel/plugin-proposal-class-properties", { "loose": true }],
		["@babel/plugin-transform-classes", {"loose": true}]
	]
  }